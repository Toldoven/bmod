config_version=5

[application]

config/name="BMOD Test"
run/main_scene="res://test.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

BMOD="*res://addons/bmod/autoloads/bmod/bmod.tscn"

[debug]

